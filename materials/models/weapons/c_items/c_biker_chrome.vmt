"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\c_items\c_biker_chrome"

	$halflambert 0
	//"$phongexponenttexture" "models\weapons\c_items/c_biker_chrome_phongexponent"
	"$normalmapalphaenvmapmask" 1
	"$basealphaenvmapmask" 1 
	"$basemapalphaphongmask" 1
	"$phong" "1"
	"$phongexponent" "2"
	"$phongboost" "12"	
	"$lightwarptexture" "models\weapons\c_items/c_ambassador_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$phongwarptexture"  "models\weapons\c_items/c_ambassador_phongwarp"


	"$rimlight" "0.5"
	"$rimlightexponent" "2"	
	"$rimlightboost" "3"

	"360?$color2" "[ 0.9 0.8 0.8 ]"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
