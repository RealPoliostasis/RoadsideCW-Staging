"VertexlitGeneric"
{
	"$basetexture" "vgui\replay\thumbnails\models/weapons/c_models/c_shotgun/broken_steel_stock_01"
			
		 "$phong" "1"
		 "$phongexponent" "5"
		 "$phongboost" "5"	
	     "$lightwarptexture" "models\lightwarps\weapon_lightwarp"
         "$phongfresnelranges"	"[1 1 1]"


	"$phongExponentTexture" "vgui\replay\thumbnails\models/weapons/c_models/c_shotgun/specular_stock_01"
	
	$phongAlbedoTint 1

//SheenColor
     $phongwarptexture "vgui\replay\thumbnails\models/weapons/c_models/c_shotgun/sheen_6"


	"$rimlight" "1"						
	"$rimlightexponent" "1"				
	"$rimlightboost" "6"

//Cubemap stuff
	"$envmap" "env_cubemap"	
	"$envmaptint" "[ .4 .4 .4 ]"

//Jaraterfix?: //sheenfix:
	"$halflambert" "1"

"$glowcolor" "1"


"$tempvar" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}
